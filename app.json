{
  "expo": {
    "name": "plant-manager-app",
    "slug": "plant-manager-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.nicolasoliveira861.plantmanagerapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "React Native project designed to manage your plants!",
    "githubUrl": "https://github.com/NicolasOliveira861/plant-manager-app"
  }
}
